{
  "name": "cloudflare-mcp",
  "owner": {
    "name": "Cloudflare Community",
    "email": "community@cloudflare.com"
  },
  "metadata": {
    "description": "Claude Code plugins for Cloudflare. Choose by workflow: dev, security, ai, or analytics.",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "cloudflare",
      "source": "./",
      "description": "All 15 Cloudflare MCP servers (for power users who need everything)",
      "version": "1.0.0",
      "author": { "name": "Cloudflare, Inc." },
      "repository": "https://github.com/schmug/cloudflare-claude-plugin",
      "license": "Apache-2.0",
      "keywords": ["cloudflare", "workers", "kv", "r2", "d1", "mcp", "all"],
      "category": "cloud-infrastructure",
      "mcpServers": "./.mcp.json"
    },
    {
      "name": "cloudflare-docs",
      "source": "./",
      "description": "Search Cloudflare documentation (no API token required)",
      "version": "1.0.0",
      "author": { "name": "Cloudflare, Inc." },
      "license": "Apache-2.0",
      "keywords": ["cloudflare", "docs", "documentation"],
      "category": "documentation",
      "mcpServers": {
        "cloudflare-docs": {
          "type": "http",
          "url": "https://docs.mcp.cloudflare.com/mcp"
        }
      },
      "strict": false
    },
    {
      "name": "cloudflare-dev",
      "source": "./",
      "description": "Workers/Pages development: storage (KV, R2, D1), deployments, logs, and sandbox containers",
      "version": "1.0.0",
      "author": { "name": "Cloudflare, Inc." },
      "license": "Apache-2.0",
      "keywords": ["cloudflare", "workers", "pages", "kv", "r2", "d1", "deploy", "logs"],
      "category": "development",
      "mcpServers": {
        "cloudflare-bindings": {
          "type": "http",
          "url": "https://bindings.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-builds": {
          "type": "http",
          "url": "https://builds.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-observability": {
          "type": "http",
          "url": "https://observability.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-containers": {
          "type": "http",
          "url": "https://containers.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        }
      },
      "strict": false
    },
    {
      "name": "cloudflare-security",
      "source": "./",
      "description": "Security operations: URL scanning, threat intel (Radar), SaaS security (CASB), and audit logs",
      "version": "1.0.0",
      "author": { "name": "Cloudflare, Inc." },
      "license": "Apache-2.0",
      "keywords": ["cloudflare", "security", "radar", "casb", "audit", "compliance"],
      "category": "security",
      "mcpServers": {
        "cloudflare-radar": {
          "type": "http",
          "url": "https://radar.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-casb": {
          "type": "http",
          "url": "https://casb.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-auditlogs": {
          "type": "http",
          "url": "https://auditlogs.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        }
      },
      "strict": false
    },
    {
      "name": "cloudflare-ai",
      "source": "./",
      "description": "AI development: AI Gateway logs, AutoRAG document search, and Browser Rendering",
      "version": "1.0.0",
      "author": { "name": "Cloudflare, Inc." },
      "license": "Apache-2.0",
      "keywords": ["cloudflare", "ai", "ai-gateway", "autorag", "browser", "llm"],
      "category": "ai",
      "mcpServers": {
        "cloudflare-ai-gateway": {
          "type": "http",
          "url": "https://ai-gateway.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-autorag": {
          "type": "http",
          "url": "https://autorag.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-browser": {
          "type": "http",
          "url": "https://browser.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        }
      },
      "strict": false
    },
    {
      "name": "cloudflare-analytics",
      "source": "./",
      "description": "Deep analytics: GraphQL API, DNS analytics, DEX monitoring, and Logpush",
      "version": "1.0.0",
      "author": { "name": "Cloudflare, Inc." },
      "license": "Apache-2.0",
      "keywords": ["cloudflare", "analytics", "graphql", "dns", "dex", "logs"],
      "category": "analytics",
      "mcpServers": {
        "cloudflare-graphql": {
          "type": "http",
          "url": "https://graphql.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-dns-analytics": {
          "type": "http",
          "url": "https://dns-analytics.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-dex": {
          "type": "http",
          "url": "https://dex.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        },
        "cloudflare-logs": {
          "type": "http",
          "url": "https://logs.mcp.cloudflare.com/mcp",
          "headers": { "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}" }
        }
      },
      "strict": false
    }
  ]
}
